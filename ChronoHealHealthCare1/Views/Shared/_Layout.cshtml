<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title - ChronoHeal Health Care</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />

    <!-- Favicon -->
    <!--<link href="img/favicon.ico" rel="icon">-->
    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Roboto+Condensed:wght@400;700&family=Roboto:wght@400;700&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    @*Libraries Stylesheet
        <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
        <link href="lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />*@

    <link href="~/css/bootstrap.min.css" rel="stylesheet">

    @*<script src="https://cdn.tailwindcss.com"></script>*@

    <!-- Template Stylesheet -->
    <link href="~/Content/style.css" rel="stylesheet">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    
</head>

<body style="font-family:'Century Gothic'; background-attachment:fixed">
    <!-- Topbar Start -->
    <div class="container-fluid py-2 border-bottom d-none d-lg-block">
        <div class="container">
            <div class="row">
                <div class="col-md-6 text-center text-lg-start mb-2 mb-lg-0">
                    <div class="d-inline-flex align-items-center hov1" style="margin-top:10px; padding: 10px; border-radius:30px">
                        <a class="text-decoration-none text-body pe-3" href=""><i class="bi bi-telephone me-2"></i>@Html.ActionLink("+91 8436220406", "Create", "Contacts", new { area = "" }, new { @class = " nav-link text-black" })</a>
                        <span class="text-body">|</span>
                        <a class="text-decoration-none text-body px-3" href=""><i class="bi bi-envelope me-2"></i>@Html.ActionLink("info@chronoheal.com", "Create", "Contacts", new { area = "" }, new { @class = " nav-link text-black" })</a>
                    </div>
                </div>
                <div class="col-md-6 text-center text-lg-end">
                    <div class="d-inline-flex align-items-center">
                        @Html.Partial("_LoginPartial")
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Topbar End -->
    <!-- Navbar Start -->
    <div class="container-fluid sticky-top bg-white" style="background-image: url(/Images/nav.jpg); height:112px; box-shadow: 4px 4px 20px rgba(0,0,0,0.2), -6px -6px  20px 0px rgba(255,255,255,0.6);">
        <div class="container">
            <nav class="navbar navbar-expand-lg navbar-light py-3 py-lg-0">

                <img src="~/Images/logo1.jpg" class="logo" style="box-shadow: 4px 4px 20px rgba(0,0,0,0.2), -6px -6px  20px 0px rgba(250,250,250,0.6); height:90px; width:90px; margin-right:30px; margin-top:10px" />

                <h1 class="m-0 text-dark" style="padding-top:8px">Chrono<span style="color:Highlight">Heal</span></h1>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse" style="box-shadow: 4px 4px 20px rgba(0,0,0,0.2), -6px -6px  20px 0px rgba(255,255,255,0.6); border-radius:40px; margin-top:10px; margin-left:30px; backdrop-filter:blur(5px)">
                    <div class="navbar-nav ms-auto py-0" style="font-family:'Century Gothic'">
                        @Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "nav-link" })
                        @Html.ActionLink("About", "About", "Home", new { area = "" }, new { @class = "nav-link" })
                        @Html.ActionLink("Doctors", "Index", "Doctors", new { area = "" }, new { @class = "nav-link" })
                        @Html.ActionLink("Departments", "Index", "Departments", new { area = "" }, new { @class = "nav-link" })
                        @Html.ActionLink("Contact Us", "Create", "Contacts", new { area = "" }, new { @class = "nav-link" })
                        @Html.ActionLink("Career Form", "Create", "Career_Registration", new { area = "" }, new { @class = "nav-link" })
                       

                    </div>


                </div>
            </nav>
        </div>
    </div>
    <!-- Navbar End -->
    @*@if (Request.IsAuthenticated)
            {
                <nav id="navbar-example2" class="navbar navbar-light bg-light">
                    <a class="navbar-brand" href="#">Admin Panel</a>
                    <div class="nav nav-pills">
                        @Html.ActionLink("Account", "Index", "Manage", new { area = "" }, new { @class = "nav-item nav-link" })
                        @Html.ActionLink("Appointments", "Create", "Appointments", new { area = "" }, new { @class = "nav-item nav-link" })
                        @Html.ActionLink("User Details", "Create", "PDetails", new { area = "" }, new { @class = "nav-item nav-link" })
                        @Html.ActionLink("Career Opportunity", "Create", "Career_Registration", new { area = "" }, new { @class = "nav-item nav-link" })
                    </div>
                </nav>
            }
            else
            {

        }*@


    @*@try {{Html.RenderAction("UserNavbar");} }
        catch { }*@

    @Html.Partial("_PanelNavbar")
    @RenderBody()
    <!-- Footer Start -->
    <div class="container-fluid text-dark mt-5 py-5 before1" style="background-color:rgba(0,0,0,0.1)">
        <div class="container py-5">
            <div class="row g-5">
                <div class="col-lg-3 col-md-6">
                    <h4 class="d-inline-block text-uppercase border-bottom border-5 border-secondary mb-4">Get In Touch</h4>
                    <p class="mb-4">If you have an emergency, you can contact us using these details given below, or if you have any query you can send us a message in the Contact section</p>
                    <p class="mb-2"><i class="fa fa-map-marker-alt text-light me-3"></i>123 Street, Bengaluru, India</p>
                    <p class="mb-2"><i class="fa fa-envelope text-light me-3"></i>info@chronoheal.com</p>
                    <p class="mb-0"><i class="fa fa-phone-alt text-light me-3"></i>+91 8436220406</p>
                </div>
                <div class="col-lg-3 col-md-6">
                    <h4 class="d-inline-block text-uppercase border-bottom border-5 border-secondary mb-4">Quick Links</h4>
                    <div class="d-flex flex-column justify-content-start">
                        <p class="mb-2" href="#"><i class="fa fa-angle-right me-2"></i>@Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "nav-item text-dark" })</p>
                        <p class="mb-2" href="#"><i class="fa fa-angle-right me-2"></i>@Html.ActionLink("About Us", "About", "Home", new { area = "" }, new { @class = "nav-item text-dark" })</p>
                        <p class="mb-2" href="#"><i class="fa fa-angle-right me-2"></i>@Html.ActionLink("Doctors", "Index", "Doctors", new { area = "" }, new { @class = "nav-item text-dark" })</p>
                        <p class="mb-2" href="#"><i class="fa fa-angle-right me-2"></i>@Html.ActionLink("Departments", "Index", "Departments", new { area = "" }, new { @class = "nav-item text-dark" })</p>
                        <p class=""><i class="fa fa-angle-right me-2"></i>@Html.ActionLink("Contact Us", "Create", "Contacts", new { area = "" }, new { @class = "nav-item text-dark" })</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <h4 class="d-inline-block text-uppercase border-bottom border-5 border-secondary mb-4">Popular Links</h4>
                    <div class="d-flex flex-column justify-content-start">
                        <p class="mb-2"><i class="fa fa-angle-right me-2"></i>@Html.ActionLink("Appointment", "Create", "Appointments", new { area = "" }, new { @class = "nav-item text-dark" })</p>
                        <p class="mb-2"><i class="fa fa-angle-right me-2"></i>@Html.ActionLink("Career", "Create", "Career_Registration", new { area = "" }, new { @class = "nav-item text-dark" })</p>
                        <p class="mb-2"><i class="fa fa-angle-right me-2"></i>@Html.ActionLink("Sign-up", "Register", "Account", new { area = "" }, new { @class = "nav-item text-dark" })</p>
                        <p class="mb-2"><i class="fa fa-angle-right me-2"></i>@Html.ActionLink("Sign-in","Login","Account", new { area = "" }, new { @class = "nav-item text-dark" })</p>
                        <p class=""><i class="fa fa-angle-right me-2"></i>@Html.ActionLink("Account", "Index", "Manage", new { area = "" }, new { @class = "nav-item text-dark" })</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <h4 class="d-inline-block text-uppercase border-bottom border-5 border-secondary mb-4">Social</h4>

                    <div class="input-group">
                        @Html.Partial("_LoginPartial")
                    </div>

                    <h6 class="text-uppercase mt-4 mb-3">Follow Us</h6>
                    <div class="d-flex">
                        <a class="btn btn-lg btn-primary btn-lg-square rounded-circle me-2" style="box-shadow: 5px 5px 10px rgba(0,0,0,0.2), -5px -5px 10px 0px rgba(255,255,255,1)" href="#"><i class="fab fa-twitter"></i></a>
                        <a class="btn btn-lg btn-primary btn-lg-square rounded-circle me-2" style="box-shadow: 5px 5px 10px rgba(0,0,0,0.2), -5px -5px 10px 0px rgba(255,255,255,1)" href="#"><i class="fab fa-facebook-f"></i></a>
                        <a class="btn btn-lg btn-primary btn-lg-square rounded-circle me-2" style="box-shadow: 5px 5px 10px rgba(0,0,0,0.2), -5px -5px 10px 0px rgba(255,255,255,1)" href="#"><i class="fab fa-linkedin-in"></i></a>
                        <a class="btn btn-lg btn-primary btn-lg-square rounded-circle me-2" style="box-shadow: 5px 5px 10px rgba(0,0,0,0.2), -5px -5px 10px 0px rgba(255,255,255,1)" href="#"><i class="fab fa-instagram"></i></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container-fluid bg-dark text-light border-top border-secondary py-4">
        <div class="container">
            <div class="row g-5">
                <div class="col-md-6 text-center text-md-start">
                    <p class="mb-md-0">&copy; <span>@Html.ActionLink("ChronoHeal Health Care", "Index", "Home", new { area = "" }, new { @class = "nav-item text-white-50" })</span>. All Rights Reserved.</p>
                </div>
                <div class="col-md-6 text-center text-md-end">
                    <p class="mb-0">Designed by <span style="color:aquamarine">Akashdeep</span></p>
                </div>
            </div>
        </div>
    </div>
    <!-- Footer End -->
    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/waypoints/waypoints.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="lib/tempusdominus/js/moment.min.js"></script>
    <script src="lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="js/main.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>

</html>